        "type": "PREC",
        "value": 1,
        "content": {
          "type": "CHOICE",
          "members": [
            {
              "type": "STRING",
              "value": "#'"
            },
            {
              "type": "STRING",
              "value": "//'"
            }
          ]
        }
      }
