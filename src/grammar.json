      "type": "TOKEN",
      "content": {
        "type": "PREC",
        "value": -100,
        "content": {
          "type": "PATTERN",
          "value": "[^\\[\\]\\{\\}\\(\\)\\s\\n\\r]*"
        }
      }
    },
